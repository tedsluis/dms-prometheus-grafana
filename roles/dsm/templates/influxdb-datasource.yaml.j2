aapiVersion: 1

datasources:
  - name: InfluxDB
    type: influxdb
    access: proxy
    database: luftdaten
    user: influxdb
    password: {{_influxdb_password}}
    url: http://{{inventory_hostname}}:8086
    jsonData:
      httpMode: GET
    secureJsonData:
      password: {{_influxdb_password}}